<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import java.net.URL?>
<AnchorPane id="root" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.itis.snaky.client.controllers.RoomsWindowController">

    <ListView fx:id="listView" onMouseClicked="#roomChosen" prefHeight="100.0"
              style="-fx-border-radius: 10%; -fx-background-color: #5D9400;" AnchorPane.bottomAnchor="100.0"
              AnchorPane.leftAnchor="100.0" AnchorPane.topAnchor="100.0"/>
    <stylesheets>
        <URL value="@/css/main.css"/>
    </stylesheets>
</AnchorPane>
